{
    "name": "tysonandre/phantypocheck",
    "description": "A plugin for Phan that checks for typos (using codespell's dictionary).",
    "keywords": ["phan", "gettext", "phan-plugin", "typo", "codespell"],
    "type": "project",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Tyson Andre"
        }
    ],
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.2.31"
        }
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phan/phan": "^2.5.0 || ^3.0.0 || ^4.0.0"
    },
    "bin": ["phptypocheck"]
}
